{
  "mcpServers": {
    "session-mgmt": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/les/Projects/session-mgmt-mcp",
        "python",
        "session_mgmt_mcp/server.py"
      ]
    },
    "crackerjack": {
      "command": "python",
      "args": [
        "-m",
        "crackerjack",
        "--start-mcp-server"
      ],
      "env": {
        "UV_KEYRING_PROVIDER": "subprocess",
        "EDITOR": "code --wait"
      }
    },
    "claude-simone": {
      "command": "npx",
      "args": ["--yes", "simone-mcp@latest"],
      "env": {
        "PROJECT_PATH": "/Users/les/Projects/crackerjack"
      }
    },
    "ast-grep": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/les/Projects/claude/mcp-servers/ast-grep-mcp",
        "run",
        "main.py"
      ],
      "env": {
        "AST_GREP_CONFIG": "/Users/les/Projects/claude/mcp-servers/ast-grep-mcp/sgconfig.yaml"
      }
    },
    "macos_automator": {
      "command": "npx",
      "args": [
        "-y",
        "@steipete/macos-automator-mcp@latest"
      ]
    },
    "peekaboo": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@steipete/peekaboo-mcp@beta"
      ],
      "env": {
        "PEEKABOO_AI_PROVIDERS": "anthropic/claude-opus-4,anthropic/claude-sonnet-4"
      }
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@cyanheads/git-mcp-server"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ]
    },
    "obsidian": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://localhost:27123/sse"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "filesystem": {
      "command": "rust-mcp-filesystem",
      "args": [
        "~/Projects"
      ]
    },
    "jetbrains": {
      "command": "npx",
      "args": [
        "-y",
        "@jetbrains/mcp-proxy"
      ]
    },
    "gemini-cli": {
      "command": "npx",
      "args": [
        "-y",
        "gemini-mcp-tool"
      ]
    },
    "upstash": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/mcp-server",
        "run"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "cloud-run": {
      "command": "npx",
      "args": [
        "-y",
        "https://github.com/GoogleCloudPlatform/cloud-run-mcp"
      ]
    },
    "logfire": {
      "command": "uvx",
      "args": [
        "logfire-mcp@latest"
      ]
    },
    "raindrop": {
      "command": "npx",
      "args": [
        "-y",
        "raindrop-mcp"
      ]
    },
    "turso-cloud": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-turso-cloud"
      ]
    },
    "motherduck": {
      "command": "uvx",
      "args": [
        "mcp-server-motherduck",
        "--db-path",
        "~/Projects/claude/shared_analytics.duckdb",
        "--read-only",
        "--transport",
        "stdio"
      ],
      "env": {
        "HOME": "/Users/les"
      }
    },
    "sentry": {
      "type": "http",
      "url": "https://mcp.sentry.dev/mcp"
    }
  }
}
