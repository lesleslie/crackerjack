{
  "mcpServers": {
    "session-mgmt": {
      "command": "uvx",
      "args": [
        "--from",
        "/Users/les/Projects/session-mgmt-mcp",
        "session-mgmt-mcp"
      ],
      "env": {}
    },
    "ast-grep": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/ast-grep/ast-grep-mcp",
        "ast-grep-server"
      ]
    },
    "crackerjack": {
      "command": "uvx",
      "args": [
        "--from",
        "/Users/les/Projects/crackerjack",
        "crackerjack",
        "--start-mcp-server"
      ],
      "env": {
        "UV_KEYRING_PROVIDER": "subprocess",
        "EDITOR": "code --wait"
      }
    },
    "macos_automator": {
      "command": "npx",
      "args": [
        "-y",
        "@steipete/macos-automator-mcp@0.4.1"
      ]
    },
    "peekaboo": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@steipete/peekaboo-mcp@3.0.0-beta.2"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server@1.0.6"
      ]
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@cyanheads/git-mcp-server@2.3.2"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github@2025.4.8"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN_USE_GHCLI": "true"
      },
      "_comment": "Package deprecated - consider migrating to github.com/github/github-mcp-server"
    },
    "gitlab": {
      "command": "npx",
      "args": [
        "-y",
        "@zereight/mcp-gitlab@2.0.3"
      ]
    },
    "obsidian": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote@0.1.18",
        "http://localhost:27123/sse"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory@2025.8.4"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking@2025.7.1"
      ]
    },
    "filesystem": {
      "command": "rust-mcp-filesystem",
      "args": [
        "-w",
        "~/Projects"
      ]
    },
    "jetbrains": {
      "command": "npx",
      "args": [
        "-y",
        "@jetbrains/mcp-proxy@1.8.0"
      ]
    },
    "gemini-cli": {
      "command": "npx",
      "args": [
        "-y",
        "gemini-mcp-tool@1.1.4"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@1.0.16"
      ]
    },
    "raindrop": {
      "command": "npx",
      "args": [
        "-y",
        "raindrop-mcp@0.0.4"
      ]
    },
    "excalidraw": {
      "command": "uvx",
      "args": [
        "excalidraw-mcp"
      ],
      "env": {
        "EXPRESS_SERVER_URL": "http://localhost:3031",
        "ENABLE_CANVAS_SYNC": "true"
      }
    },
    "motherduck": {
      "command": "uvx",
      "args": [
        "mcp-server-motherduck==0.3.1"
      ]
    }
  }
}
